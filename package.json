{
  "name": "upwork-proposal-chatbot",
  "version": "1.0.0",
  "description": "A chatbot that generates 3 proposals based on Upwork project descriptions using ChatGPT API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["chatbot", "upwork", "proposals", "chatgpt", "api"],
  "author": "Your Name",
  "license": "MIT"
}
